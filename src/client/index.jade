doctype html
html
  head
    title "Pcv - Portafolios Modernos"
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
  body
    //App container Component
    section.App-container
      include header/index.jade
      include landing/index.jade
      include cover-letter/index.jade
      include educations/index.jade
      section.Skills
        ul.Skills-list
          each skill in [1,2,3,4,5]  
            li.Skills-skill
              div.Skill-left
                span.Skill-label HTML5
              div.Skill-right
                span.Skill-value 100%
              
      
